{
  "version": 1,
  "projects": {
    "gitlab-helper": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/gitlab-helper",
      "sourceRoot": "apps/gitlab-helper/src",
      "prefix": "gitlab-helper",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/gitlab-helper/browser",
            "index": "apps/gitlab-helper/src/index.html",
            "main": "apps/gitlab-helper/src/main.ts",
            "polyfills": "apps/gitlab-helper/src/polyfills.ts",
            "tsConfig": "apps/gitlab-helper/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/gitlab-helper/src/favicon.ico",
              "apps/gitlab-helper/src/assets",
              {
                "glob": "robots.txt",
                "input": "./resources/assets",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "./resources/assets",
                "output": "/assets/"
              },
              {
                "glob": "config.json",
                "input": "apps/gitlab-helper/src/config/",
                "output": "/"
              }
            ],
            "styles": [
              "./node_modules/normalize.css/normalize.css",
              "resources/styles/styles.scss",
              "apps/gitlab-helper/src/styles.scss",
              {
                "input": "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
              }
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/gitlab-helper/src/environments/environment.ts",
                  "with": "apps/gitlab-helper/src/environments/environment.prod.ts"
                },
                {
                  "replace": "apps/gitlab-helper/src/config/config.json",
                  "with": "apps/gitlab-helper/src/config/config.prod.json"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "gitlab-helper:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "gitlab-helper:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "gitlab-helper:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/gitlab-helper/tsconfig.app.json", "apps/gitlab-helper/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/gitlab-helper/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/gitlab-helper/jest.config.js",
            "tsConfig": "apps/gitlab-helper/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/gitlab-helper/src/test-setup.ts"
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/gitlab-helper/server",
            "main": "apps/gitlab-helper/server.ts",
            "tsConfig": "apps/gitlab-helper/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "apps/gitlab-helper/src/environments/environment.ts",
                  "with": "apps/gitlab-helper/src/environments/environment.prod.ts"
                },
                {
                  "replace": "apps/gitlab-helper/src/config/config.json",
                  "with": "apps/gitlab-helper/src/config/config.prod.json"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "gitlab-helper:build",
            "serverTarget": "gitlab-helper:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "gitlab-helper:build:production",
              "serverTarget": "gitlab-helper:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "gitlab-helper:build:production",
            "serverTarget": "gitlab-helper:server:production",
            "routes": ["/"]
          },
          "configurations": {
            "production": {}
          }
        }
      }
    },
    "gitlab-helper-e2e": {
      "root": "apps/gitlab-helper-e2e",
      "sourceRoot": "apps/gitlab-helper-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/gitlab-helper-e2e/cypress.json",
            "tsConfig": "apps/gitlab-helper-e2e/tsconfig.e2e.json",
            "devServerTarget": "gitlab-helper:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "gitlab-helper:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/gitlab-helper-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/gitlab-helper-e2e/**/*"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    },
    "@nrwl/schematics:library": {
      "style": "scss"
    },
    "@nrwl/schematics:application": {
      "style": "scss",
      "framework": "angular"
    }
  },
  "defaultProject": "gitlab-helper"
}
